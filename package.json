{
  "name": "end-to-end-playwright-test-automation-framework",
  "version": "0.0.1",
  "description": "A comprehensive end-to-end test automation framework using Playwright with BDD, Allure reporting, fixtures, and utilities.",
  "keywords": [
    "playwright",
    "end-to-end",
    "test automation",
    "BDD",
    "Allure",
    "reporting",
    "fixtures",
    "utils"
  ],
  "homepage": "https://github.com/hexdee606/end-to-end-playwright-test-automation-framework",
  "bugs": {
    "url": "https://github.com/hexdee606/end-to-end-playwright-test-automation-framework/issues",
    "email": "support@example.com"
  },
  "license": "CC0-1.0",
  "author": "hexdee606 <hexdee606@gmail.com> (https://github.com/hexdee606)",
  "contributors": [
    "Afroj <email@email.com>"
  ],
  "main": "index.js",
  "bin": {},
  "scripts": {
    "test:e2e": "bddgen && playwright test",
    "test:parallel": "bddgen && playwright test --workers=4 ",
    "test:smoke": "bddgen && playwright test --grep @smoke",
    "test:debug": "bddgen && playwright test -g @debug --debug",
    "test:report-serve": "allure serve ./outputs/allure-results --clean",
    "test:report-generate": "allure generate ./outputs/allure-results --clean -o ./allure-report",
    "test:report-open": "allure open ./allure-report"
  },
  "dependencies": {
    "@playwright/test": "^1.46.1",
    "exceljs": "^4.4.0",
    "playwright": "^1.46.1",
    "playwright-lighthouse": "^4.0.0"
  },
  "devDependencies": {
    "allure-playwright": "^3.0.0-beta.11",
    "playwright-bdd": "^7.2.2",
    "allure-commandline": "^2.30.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hexdee606/end-to-end-playwright-test-automation-framework.git"
  }
}
